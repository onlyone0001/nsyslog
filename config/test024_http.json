{
	"config" : {
		"datadir" : "/tmp/nsyslog"
	},

	"inputs" : {
		"http" : {
			"type" : "http",
			"config" : {
				"url" : "https://jsonplaceholder.typicode.com/todos/1",
				"interval" : 2000,
				"options" : {},
				"tls" : {
					"rejectUnauthorized" : false
				}
			}
		},
		"httpauth" : {
			"type" : "http",
			"config" : {
				"url" : "https://jigsaw.w3.org/HTTP/Basic/",
				"interval" : 2000,
				"options" : {
					"auth" : {
						"user" : "guest",
						"password" : "guest"
					}
				}
			}
		}
	},

	"processors" : {
		"json" : {
			"type" : "jsonparser",
			"config" : {
				"field" : "${originalMessage}",
				"output" : "originalMessage"
			}
		}
	},

	"transporters" : {
		"console" : {
			"type" : "console",
			"config" : {
				"format" : "${JSON}"
			}
		}
	},

	"flows" : [
		{"from":"http", "processors":"json", "transporters":"console"},
		{"from":"httpauth", "transporters":"console"}
	]
}
