{
	"register" : [
		{"type":"input","id":"mypush","require":"custom/push-input.js"},
		{"type":"input","id":"mypull","require":"custom/pull-input.js"},
		{"type":"processor","id":"myproc","require":"custom/custom-processor.js"},
		{"type":"transporter","id":"mytrans","require":"custom/custom-transporter.js"},
		{"type":"transporter","id":"mytransblock","require":"custom/custom-transporter-block.js"}
	],

	"config" : {
		"datadir" : "/tmp/nsyslog",
		"input" : {"maxPending" : 100},
		"buffer" : {"maxPending" : 100},
		"processor" : {"maxPending" : 100},
		"transporter" : {"maxPending" : 100}

	},

	"inputs" : {
		"pusher" : {
			"type" : "mypull",
			"config" : {
				"interval" : 1,
				"threshold" : 1
			}
		}
	},

	"processors" : {
		"multi" : {
			"type" : "properties",
			"config" : {
				"set" : {
					"test" : 0
				}
			}
		}
	},

	"transporters" : {
		"block" : {
			"type" : "mytransblock",
			"config" : {
				"format" : "${seq}: ${originalMessage}"
			}
		}
	},

	"flows" : [
		{"from":"*", "processors":"multi","transporters":["block"], "mode":"parallel"}
	]
}
