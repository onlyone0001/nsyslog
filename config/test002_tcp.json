{
	"config" : {
		"datadir" : "/tmp/nsyslog",
		"input" : {"buffer" : 1000},
		"process" : {"buffer": 1000}
	},

	"inputs" : {
		"tcp" : {
			"type" : "tcp",
			"config" : {}
		}
	},

	"processors" : {
		"timestamp" : {
			"type" : "timestamp",
			"config" : {}
		}
	},

	"transporters" : {
		"file" : {
			"type" : "file",
			"config" : {
				"format" : "${originalMessage}",
				"path" : "/var/log/nsyslog/test002_out.log"
			}
		},
		"log" : {
			"type" : "console",
			"config" : {"format" : "${originalMessage}", "level" : "log"}
		}
	},

	"flows" : [
		{"from":"${input}=='glob'", "processors":["timestamp"], "transporters":["log","file"], "mode":"parallel"}
	]
}
