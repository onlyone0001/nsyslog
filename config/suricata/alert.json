{
  "processors": {
    "alert_event": {
      "type": "properties",
      "config": {
				"extend" : false,
        "set": {
					"dbcol" : "${dbcol}",
					"timestamp" : "${ts}",
					"et" : "${timestamp}",
					"evtdate" : "${evtdate}",
					"evttime" : "${evttime}",
					"type" : "${etype}",
					"res" : "${res}",
					"subres" : "${in_iface}",
          "st" : "${st}",
          "sip": "${src_ip}",
          "sp" : "${src_port}",
          "dip" : "${dest_ip}",
          "dp" : "${dest_port}",
          "p" : "${proto}",
					"pn" : "${app_proto}",
					"sev" : "${alert.severity}",
					"basemsg" : "${alert.signature}",
					"ct1" : "${alert.action}",
					"ct2" : "${alert.gid}",
					"ct3" : "${alert.signature_id}",
					"ct4" : "${alert.rev}",
					"ct5" : "${alert.category}",
					"ct6" : "${app_proto}",
					"ei" : {
						"alert" : "${alert}"
					}
        }
      }
    }
  },
  "flows": [
    {"from": "${etype}=='alert'", "processors": "alert_event", "transporters":"$publish"}
  ]
}
