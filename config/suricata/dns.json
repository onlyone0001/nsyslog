{
  "processors": {
    "dns_event": {
      "type": "properties",
      "config": {
				"extend" : false,
        "set": {
					"dbcol" : "${dbcol}",
					"timestamp" : "${ts}",
					"et" : "${timestamp}",
					"evtdate" : "${evtdate}",
					"evttime" : "${evttime}",
					"type" : "${etype}",
					"res" : "${res}",
					"subres" : "${in_iface}",
          "st" : "${st}",
          "sip": "${src_ip}",
          "sp" : "${src_port}",
          "dip" : "${dest_ip}",
          "dp" : "${dest_port}",
          "p" : "${proto}",
					"ct1" : "${dns.type}",
					"ct2" : "${dns.id}",
					"ct3" : "${dns.rrname}",
					"ct4" : "${dns.rrtype}",
					"ct5" : "${dns.tx_id}",
					"ei" : {
						"dns" : "${dns}"
					}
        }
      }
    }
  },
  "flows": [
    {"from": "${etype}=='dns'", "processors": "dns_event", "transporters":"$publish"}
  ]
}
