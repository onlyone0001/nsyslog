{
  "processors": {
    "http_event": {
      "type": "properties",
      "config": {
				"extend" : false,
        "set": {
					"dbcol" : "${dbcol}",
					"timestamp" : "${ts}",
					"et" : "${timestamp}",
					"evtdate" : "${evtdate}",
					"evttime" : "${evttime}",
					"type" : "${type}",
					"res" : "${res}",
					"subres" : "${in_iface}",
          "st" : "${st}",
          "sip": "${src_ip}",
          "sp" : "${src_port}",
          "dip" : "${dest_ip}",
          "dp" : "${dest_port}",
          "p" : "${proto}",
          "fn" : "${http.filename}",
					"ct1" : "${http.hostname}",
					"ct2" : "${http.url}",
					"ct3" : "${http.http_user_agent}",
					"ct4" : "${http.method}",
					"ct5" : "${http.protocol}",
					"ct6" : "${http.status}",
					"ct7" : "${http.length}",
					"ei" : {
						"http" : "${http}"
					}
        }
      }
    }
  },
  "flows": [
    {"from": "${type}=='http'", "processors": "http_event", "transporters":"$publish"}
  ]
}
