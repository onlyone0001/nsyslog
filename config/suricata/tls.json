{
  "processors": {
    "tls_event": {
      "type": "properties",
      "config": {
				"extend" : false,								
        "set": {
					"dbcol" : "${dbcol}",
					"timestamp" : "${ts}",
					"et" : "${timestamp}",
					"evtdate" : "${evtdate}",
					"evttime" : "${evttime}",
					"type" : "${type}",
					"res" : "${res}",
					"subres" : "${in_iface}",
          "st" : "${st}",
          "sip": "${src_ip}",
          "sp" : "${src_port}",
          "dip" : "${dest_ip}",
          "dp" : "${dest_port}",
          "p" : "${proto}",
					"ct1" : "${tls.subject}",
					"ct2" : "${tls.issuerdn}",
					"ct3" : "${tls.serial}",
					"ct4" : "${tls.fingerprint}",
					"ct5" : "${tls.version}",
					"ct6" : "${tls.notbefore}",
					"ct6" : "${tls.notafter}",
					"ei" : {
						"tls" : "${tls}"
					}
        }
      }
    }
  },
  "flows": [
    {"from": "${type}=='tls'", "processors": "tls_event", "transporters":"mongo"}
  ]
}
