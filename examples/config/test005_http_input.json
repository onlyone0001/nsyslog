{
	"config" : {
		"datadir" : "/tmp/nsyslog"
	},

	"inputs" : {
		"http" : {
			"type" : "http",
			"config" : {
				"url" : "https://jsonplaceholder.typicode.com/posts/${postId+1}/comments",
				"options" : {
					"jar" : true
				},
				"tls" : {
					"rejectUnauthorized" : false
				},
				"watermark" : {"postId":0}
			}
		}
	},

	"processors" : {
	},

	"transporters" : {
		"console" : {
			"type" : "console",
			"config" : {
				"format" : "${JSON}"
			}
		}
	},

	"flows" : [
		{"from":"http", "transporters":"console"}
	]
}
