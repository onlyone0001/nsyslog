{
	"config" : {
		"datadir" : "/tmp/nsyslog2",
		"input" : {"maxPending" : 1000},
		"buffer" : {"maxPending": 1000},
		"processor" : {"maxPending": 1000},
		"transporter" : {"maxPending": 1000}
	},

	"inputs" : {
		"file" : {
			"type" : "file",
			"config" : {
				"path" : "/var/log/**/*.log",
				"watch" : true,
				"readmode" : "offset",
				"offset" : "start"
			}
		}
	},

	"processors" : {
		"fork1" : {"type":"properties",	"config":{"set":{"filename":"file1.log"}}},
		"fork2" : {"type":"properties",	"config":{"set":{"filename":"file2.log"}}},
		"fork3" : {"type":"properties",	"config":{"set":{"filename":"file3.log"}}},
		"fork4" : {"type":"properties",	"config":{"set":{"filename":"file4.log"}}},
		"fork5" : {"type":"properties",	"config":{"set":{"filename":"file5.log"}}},
		"fork6" : {"type":"properties",	"config":{"set":{"filename":"file6.log"}}},
		"fork7" : {"type":"properties",	"config":{"set":{"filename":"file7.log"}}},
		"fork8" : {"type":"properties",	"config":{"set":{"filename":"file8.log"}}},
		"fork9" : {"type":"properties",	"config":{"set":{"filename":"file9.log"}}},
		"fork10" : {"type":"properties", "config":{"set":{"filename":"file10.log"}}}
	},

	"transporters" : {
		"file" : {
			"type" : "file",
			"config" : {
				"path" : "/var/logout/${filename}.log",
				"format" : "${filename} => ${originalMessage}"
			}
		},
		"null" : {
			"type" : "null",
			"config" : {}
		}
	},

	"flows" : [
		{"id":"flow_fork1", "from":"*", "fork":false, "processors":["fork1"], "transporters":"null"},
		{"id":"flow_fork2", "from":"*", "fork":false, "processors":["fork2"], "transporters":"null"}
	]
}
