{
	"register" : [
		{"type":"input","id":"mypush","require":"examples/custom/push-input.js"},
		{"type":"input","id":"mypull","require":"examples/custom/pull-input.js"},
		{"type":"processor","id":"myproc","require":"examples/custom/custom-processor.js"},
		{"type":"processor","id":"myprocblock","require":"examples/custom/custom-processor-block.js"},
		{"type":"transporter","id":"mytrans","require":"examples/custom/custom-transporter.js"},
		{"type":"transporter","id":"mytransblock","require":"examples/custom/custom-transporter-block.js"}
	],

	"config" : {
		"datadir" : "/tmp/nsyslog",
		"input" : {"maxPending" : 100},
		"buffer" : {"maxPending" : 100},
		"processor" : {"maxPending" : 100},
		"transporter" : {"maxPending" : 100}

	},

	"inputs" : {
		"pusher" : {
			"type" : "mypull",
			"config" : {
				"interval" : 0,
				"threshold" : 1
			}
		}
	},

	"processors" : {
		"multi" : {
			"type" : "myprocblock",
			"config" : {
				"block" : false,
				"cpu" : 100000
			}
		},
		"multi2" : {
			"type" : "myprocblock",
			"config" : {
				"block" : false,
				"cpu" : 100000
			}
		}
	},

	"transporters" : {
		"block" : {
			"type" : "mytransblock",
			"config" : {
				"format" : "${seq}: ${originalMessage}",
				"block" : false
			}
		}
	},

	"flows" : [
		{"id":"fork1", "disabled":false, "from":"*", "fork":"balanced", "cores":4,  "processors":["multi","multi2"],"transporters":["block"], "mode":"parallel"},
		{"id":"fork2", "disabled":true,  "from":"*", "fork":true, "processors":["multi","multi2"],"transporters":["block"], "mode":"parallel"},
		{"id":"fork3", "disabled":true,  "from":"*", "fork":true, "processors":["multi","multi2"],"transporters":["block"], "mode":"parallel"}
	]
}
