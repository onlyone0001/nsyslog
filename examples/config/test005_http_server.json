{
	"config" : {
		"datadir" : "/tmp/nsyslog"
	},

	"inputs" : {
		"http" : {
			"type" : "httpserver",
			"config" : {
				"url" : "http://localhost:8888/store",
				"format" : "raw"
			}
		}
	},

	"processors" : {
	},

	"transporters" : {
		"console" : {
			"type" : "console",
			"config" : {
				"format" : "${JSON}"
			}
		}
	},

	"flows" : [
		{"from":"http", "transporters":"console"}
	]
}
